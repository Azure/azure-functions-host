{
    "bindings": [
        {
            "type": "eventHubTrigger",
            "direction": "in",
            "connection": "AzureWebJobsEventHubReceiver",
            "path": "%AzureWebJobsEventHubPath%"
        },
        {
            "type": "blob",
            "name": "output",
            "direction": "out",
            "path": "test-output/{id}"
        }
    ]
}
