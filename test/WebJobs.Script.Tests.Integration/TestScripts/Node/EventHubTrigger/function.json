{
    "bindings": [
        {
            "type": "eventHubTrigger",
            "name": "workItem",
            "direction": "in",
            "connection": "AzureWebJobsEventHubReceiver",
            "path": "%AzureWebJobsEventHubPath%",
            "consumerGroup": "$Default"
        },
        {
            "type": "blob",
            "name": "output",
            "direction": "out",
            "path": "test-output-node/{id}"
        }
    ]
}
