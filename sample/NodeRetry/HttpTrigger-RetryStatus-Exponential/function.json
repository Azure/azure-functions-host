{
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": 2,
    "minimumInterval": "00:00:01",
    "maximumInterval": "00:00:30"
  }
}
