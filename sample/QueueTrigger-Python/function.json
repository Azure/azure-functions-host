{
    "bindings": [
        {
            "type": "queueTrigger",
            "name": "input",
            "direction": "in",
            "queueName": "samples-python"
        },
        {
            "type": "table",
            "name": "tableInput",
            "direction": "in",
            "tableName": "samples",
            "partitionKey": "samples-python",
            "take": 5,
            "filter": "Status eq 0"
        },
        {
            "type": "blob",
            "name": "output",
            "direction": "out",
            "path": "samples-output/{id}"
        }
    ]
}
