{
  "bindings": [
    {
      "authLevel": "anonymous",
      "name": "req",
      "type": "httpTrigger",
      "route":  "counter/{countername}",
      "methods": ["put"],
      "direction": "in"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "table",
      "name": "counter",
      "tableName": "apicountertablelocal",
      "partitionKey": "items",
      "rowKey": "%rand-guid%",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}