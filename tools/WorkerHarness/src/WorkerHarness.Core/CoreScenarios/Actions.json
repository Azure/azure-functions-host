{
  "InitializeWorker": {
    "type": "InitializeWorker",
    "name": "Validating that language worker is initialize ...",
    "messages": [
      {
        "id": "message_1",
        "contentCase": "StartStream",
        "direction": "Incoming",
        "match": [
          {
            "query": "$.ContentCase",
            "expected": "StartStream"
          }
        ],
        "validators": [
          {
            "type": "regex",
            "query": "$.StartStream.WorkerId",
            "expected": "^([0-9A-Fa-f]{8}[-][0-9A-Fa-f]{4}[-][0-9A-Fa-f]{4}[-][0-9A-Fa-f]{4}[-][0-9A-Fa-f]{12})$"
          }
        ]
      },
      {
        "id": "message_2",
        "contentCase": "WorkerInitRequest",
        "direction": "Outgoing",
        "content": {}
      },
      {
        "id": "message_3",
        "contentCase": "WorkerInitResponse",
        "direction": "Incoming",
        "match": [
          {
            "query": "$.ContentCase",
            "expected": "WorkerInitResponse"
          }
        ],
        "validators": [
          {
            "type": "string",
            "query": "$.WorkerInitResponse.Result.Status",
            "expected": "Success"
          }
        ]
      }
    ]
  }
}
