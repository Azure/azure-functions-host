<Project>

  <!-- paths -->
  <PropertyGroup>
    <RepoRootPath>$(MSBuildThisFileDirectory)</RepoRootPath>
    <SourcePath>$(RepoRootPath)src</SourcePath>
    <EngPath>$(RepoRootPath)eng</EngPath>
    <ToolsPath>$(EngPath)/tools</ToolsPath>
    <ArtifactsPath>$(RepoRootPath)out</ArtifactsPath>
    <ArtifactsPublishOutputName>pub</ArtifactsPublishOutputName>
    <ArtifactsPackageOutputName>pkg</ArtifactsPackageOutputName>
    <NugetAudit>false</NugetAudit>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <Import Project="$(EngPath)targets/Release.props" />
  <Import Project="$(EngPath)targets/Version.props" />

</Project>
